{
  "hosting": {
    "public": "build/web",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/web",
        "destination": "/payment.html",
        "dynamicLinks": true
      },
      {
        "source": "**",
        "destination": "/index.html",
        "dynamicLinks": true
      }
    ]
  }
}
